# Environment variables for RAG chatbot backend
# Copy this file to .env and fill in your actual values

# OpenAI API Configuration
# Get your free API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_free_openai_api_key

# Neon Serverless PostgreSQL Database
# Get free tier from: https://neon.tech/
NEON_DB_URL=your_neon_serverless_url

# Qdrant Vector Store Configuration
# Get free cloud tier from: https://qdrant.tech/
QDRANT_API_KEY=your_qdrant_free_api_key
QDRANT_HOST=https://your_qdrant_instance_url

# Application Configuration
BACKEND_PORT=8000
FRONTEND_URL=http://localhost:3000
