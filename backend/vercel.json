{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "QDRANT_API_KEY": "@qdrant_api_key",
    "QDRANT_HOST": "@qdrant_host"
  }
}
